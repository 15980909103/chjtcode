.info{position:-webkit-sticky;position:sticky;top:0;z-index:10;margin:0 20rpx}.info .van-tab{-webkit-box-flex:0;-webkit-flex:none;flex:none;width:130rpx;font-size:34rpx;touch-action:none;color:#000;font-weight:500}.info .van-tab--active{font-size:38rpx}.info .van-sticky--fixed{top:120rpx;box-shadow:0 0 5px 1px #f8f8f8}.info .van-tabs__line{width:24rpx;bottom:18px}.info .van-tabs__content{background:#fff;overflow:hidden}.info .van-tab__pane{min-height:calc(100vh - 44px)}.new-house-activity{width:100%;height:200rpx;overflow:scroll hidden;-webkit-overflow-scrolling:touch;display:-webkit-box;display:-webkit-flex;display:flex;padding:50rpx 0 20rpx 0;background-color:#fff}.new-house-activity-item{width:208rpx;height:198rpx;padding:20rpx;background:#f0f5f6;box-sizing:border-box;margin-left:32rpx}.new-house-activity-item:last-child{margin-right:32rpx}.new-house-activity-item:nth-child(2n-1){background:#f5f8ff}.new-house-activity-item ._div{width:220rpx;font-size:30rpx;margin-bottom:10rpx}.new-house-activity-item ._img{height:110rpx;width:100%}.van-list__finished-text{padding-bottom:104rpx}.house_wrap{-webkit-transition:.3s;transition:.3s;min-height:calc(100vh - 44px)}.house_wrap_content{min-height:calc(100vh - 44px);width:100%;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-transition:.3s;transition:.3s}.house_wrap_bottom{height:102rpx;text-align:center;-webkit-transition:.3s;transition:.3s;color:hsla(0,0%,39.2%,.8);box-sizing:border-box;padding-top:10rpx}