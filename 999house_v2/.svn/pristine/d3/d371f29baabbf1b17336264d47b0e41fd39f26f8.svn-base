							this.$set(this.navList[index], 'list', data);
						}
					})
					// console.log(data,22222);
					// console.log(newData,22222);
				})
			},
			// 楼盘评论
			getTalk() {
				const id = this.houseId;
				console.log('id',id)
				console.log('houseId',this.houseId)
				const data = {
					id: id,
					page: 1,
					pageSize: 2
				};
				
				this.$http.get( 'comment/propertyReviewsList', data ).then( res=>{
					let data = res.data.list?res.data.list:[];
					const newData = [];
					
					data.map(item=>{
						const obj = {};
						obj.id = item.id;
						obj.head = this.$api.imgDirtoUrl(item.user_avatar);
						obj.name = item.user_name;
						obj.say = item.content;
						obj.time = this.$api.timeFormat(item.create_time,'yyyy年mm月dd日');
						obj.img = item.img;
						
						newData.push(obj);
					})
					
					this.navList.map((item,index)=>{
						if( item.id == 7 ){
							this.$set(this.navList[index], 'list', newData);
						}
					})
					// console.log(data);
				})
			},
			//猜你喜欢楼盘
			getEstatesLike() {
				const city_no = this.city_no;
				const data = {
					city_no: city_no,
					subject_id: this.active_id,
					estate_id: this.houseId
				};
				
				this.$http.get( 'estates/getGuessList', data ).then( res=>{
					let data = res.data;
					//console.log('猜你喜欢',data);
					if( data.length > 0 ){
						// const obj = {
						// 	id: 9,
						// 	name: '猜你喜欢',
						// 	title: '猜你喜欢',
						// 	list: this.$api.createHouseList({list:data})
						// }
						// this.navList.push(obj)
						this.likeList = this.$api.createHouseList({list:data});
					}
				})
			},
			// 关注
			careInfo() {
				this.alertLogin()
				
				this.$http.post( 'user/attentionListings', {
					id: this.houseId
				}).then( res=>{
					this.careState = this.careState == 1 ? 0 : 1;
					if(res.code==1){
						this.$toast(this.careState==1?'关注成功':'取消关注');
					}else{
						this.$toast(res.msg);
					}
				})
			},
			// 拨打电话
			callByPhone(phone) {
				uni.makePhoneCall({
				    phoneNumber: phone
				});
			}
		},
		onPageScroll(e) {
			this.scrollTop = e.scrollTop;
		}
	}

