			// 图表
			chartCreat(canvasId,data) {
				if( data.length == 0 ){
					this.haveCharts = false;
					return;
				}
				
				const that = this;
				const cWidth = uni.upx2px(686);
				const cHeight = uni.upx2px(430);
				const obj = {
					name: '',
					data: []
				};
				
				const chartData = {
					categories: [],
					series: []
				};
				
				let min = 0;
				let max = 0;
				
				data.map(item=>{
					chartData.categories.push(item.date);
					obj.name = item.type;
					obj.data.push(Number(item.value));
				})
				
				chartData.series.push(obj);
				
				// const chartData = {
				// 	categories: ['2020-01','2020-02','2020-03','2020-04','2020-05','2020-06','2020-07','2020-08','2020-09','2020-10','2020-11','2020-12'],
				// 	series: [
				// 		{
				// 			name: "本楼盘房价",
				// 			data: [10000,20000,30000,40000,55000,44000,33000,50000,40000,58000,60000,58900]
				// 		}
				// 	]
				// };
				
				min = Math.min(...chartData.series[0].data);
				max = Math.max(...chartData.series[0].data);
				
				if( min < 10000 ){
					min = 0;
				} else {
					min = (String(min)[0] - 1) * Math.pow(10,(String(min).length)-1);
				}

				max = (Number(String(max)[0]) + 1) * Math.pow(10,(String(max).length)-1);
				
				const query = uni.createSelectorQuery();
				
				query.select('#'+canvasId)
					.fields({ node: true, size: true })
					.exec((res) => {
						const canvas = res[0].node;
						const ctx = canvas.getContext('2d');
						canvas.width = res[0].width*4;
						canvas.height = res[0].height*4;
						ctx.scale(4, 4);
						
						const option = {
							$this: that,
							canvasId: canvasId,
							canvas2d: true,
							context: ctx,
							type: 'line',
							colors: ['rgba(254, 130, 30, 1)'],
							fontSize: 10,
							padding: [0,15,0,0],
							legend:{
								position: 'top',
								float: 'right',
								padding: 0,
								margin:15,
								fontSize: 12,
								fontColor: '#777'
							},
							dataLabel: false,
							dataPointShape: true,
							background:'#FFFFFF',
							categories: chartData.categories,
							series: chartData.series,
							animation: false,
							xAxis: {
								labelCount: 6,
								disableGrid: true,
							},
							yAxis: {
								data: [{
									format: val=>{
										if( val != 0 ){
											return (val/10000).toFixed(2) + '万';
										}
										
										return val;
									},
									min: min,
									max: max,
									axisLine: false,
								}],
								gridType:'dash',
								gridColor:'#CCCCCC',
								dashLength: 8,
								boundaryGap: 'justify'
							},
							width: cWidth,
							height: cHeight,
							extra: {
								line:{
									type: 'straight'
								}
							}
						};
						
						canvaLine = new uCharts(option);
					})
			},
			showLine(e){
				// console.log(887)
				canvaLine.showToolTip(e, {
					format: function (item, category) {
						return category + ' ' + item.name + ':' + (item.data/10000).toFixed(2) + '万'
					}
				});
			},
