.custom-content-marker {
	background-color: #FE821E;
	border: 1px solid #FE821E;
	color: #fff;
	text-align: center;
	box-shadow: 0px 0px 5px #E4C0A0;
}

.optionBox {
	display: flex;
	justify-content: space-between;
	margin-bottom: .15rem;
}

.option {
	color: #5A6066;
}

#container {
	height: 6rem;
}

.checked {
	color: #FE821E;
}

.transitBox {
	max-height: 1rem;
	min-height: 2rem;
	overflow: auto;
	margin-top: .15rem;
}

.contentBox {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	/* padding: .15rem 0; */
	border-top: 1px solid #F0F0F0;
	color: #5A6066;
}

.detail {
	background: #FEFFFE;
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	z-index: 999;
}

.detail_item{
	padding: .15rem 0;
	display: flex;
	justify-content: space-between;
}

.detail_no{
	width: 100%;
	min-height: 2rem;
	display: flex;
	justify-content: center;
	align-items: center;
	color: rgba(0,0,0,.5);
}

.btn {
	text-align: center;
	color: #212121;
	font-weight: bold;
	padding: .3rem;
	border-top: 1px solid #EBEBEB;
	width: 80%;
	margin: 0 auto;
}

.btn img {
	width: 0.4rem;
	vertical-align: middle;
	position: relative;
	bottom: 3px;
}

.detail .main {
	width: 80%;
	margin: 0 auto;
	padding: 0.3rem 0;
	position: relative;

}

.main-imgBox,
.main-data {
	display: inline-block;
	vertical-align: middle;
}

.main-imgBox img {
	width: 100%;
}

.main-data {
	margin-left: 0.2rem;
}

.main-title {
	font-size: 18px;
	color: #212121;
	font-weight: bold;
	margin-bottom: .15rem;
}

.main-label {
	margin-bottom: .15rem;
}

.label {
	color: #ADADAD;
	border: 1px solid #E0E0E0;
	padding: 0 0.07rem;
	margin-right: 0.07rem;
	border-radius: 0.05rem;
}

.main-location {
	color: #757575;
	font-size: 16px;
	margin-bottom: .15rem;
}

.main-price {
	color: #FC4D39;
	font-size: 18px;
	font-weight: bold;
}

#panel {
	position: fixed;
	background-color: white;
	max-height: 90%;
	overflow-y: auto;
	top: 10px;
	right: 10px;
	width: 280px;
	border-bottom: solid 1px silver;
}

.more {
	height: .6rem;
	display: flex;
	align-items: flex-end;
	justify-content: center;
}


.positionIcon {
	position: absolute;
	width: 0.2rem;
	height: 0.2rem;
	left: 47%;
	transform: rotate(45deg);
	z-index: 1;
}

.amap-marker:nth-of-type(1){
	z-index: 13 !important;
}