/* 资讯/广告/视频 样式模板 */
.all-template{
	width: 100%;
}

.template{
	width: 100%;
	box-sizing: border-box;
	padding: 0.34rem 0.32rem 0.12rem;
	display: flex;
	flex-direction: column;
	position: relative;
}

.template::after{
	content: '';
	width: 90%;
	height: 1px;
	background-color: rgba(240, 240, 240, 1);
	position: absolute;
	bottom: 0;
}

.template-name{
	font-size: .34rem;
	font-weight: bold;
	color: #000000 !important;
}

.template-title{
	font-size: 0.3rem;
	line-height: .45rem;
	color: rgba(33, 33, 33, 1);
	text-align: justify;
	text-justify: newspaper;
	word-break: break-all;
}

.template-title span{
	display: inline-block !important;
	margin-top: 0 !important;
	vertical-align: text-bottom;
}

.template-title span span{
	display: flex !important;
	margin-right: 0.04rem;
}

.template-title img{
	width: 0.26rem !important;
	height: 0.3rem !important;
}

.template-title-write{
	height: .3rem;
	font-size: 0.2rem;
	color: #fff;
	padding: 0.03rem 0.1rem;
	background: #FD4243;
	border-radius: 0.02rem;
	vertical-align: middle;
	display: flex;
	line-height: .3rem;
}

.template-news-1{
	display: flex;
	justify-content: space-between;
}

.template-news-1 .template-title{
	width: 4.4rem;
	line-height: .45rem;
}

.template-news-2 span,
.template-news-3 span,
.template-ad-1 span{
	display: flex;
	/* justify-content: space-between; */
	justify-content: flex-start;
	margin-top: 0.24rem;
}

.template-ad-1 span img{
	width: 100%;
	height: 3.86rem;
	border-radius: 0.04rem;
	object-fit: cover;
}

.template-ad-video{
	width: 100%;
	height: 3.86rem;
	margin-top: 0.24rem;
	border-radius: 0.04rem;
}

.template-news-2 span img{
	width: 49%;
	height: 2.53rem;
	border-radius: 0.04rem;
}

.template-news-3 span img{
	width: 2.18rem;
	height: 2.18rem;
	margin-right: .16rem;
	object-fit: cover;
	/* width: 32%; */
	/* height: 1.65rem; */
	/* margin-right: 1%; */
}

.template-top-1-pic,
.template-top img{
	width: 2.18rem;
	height: 1.64rem;
	object-fit: cover;
	/* width: 2rem;
	height: 1.5rem; */
	border-radius: 0.04rem;
}

.template-bottom{
	font-size: 0.24rem;
	color: rgba(173, 173, 173, 1);
	margin-top: 0.36rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.template-bottom img{
	width: 0.36rem;
	height: 0.36rem;
	border-radius: 50%;
	margin-right: 0.16rem;
}

.template-bottom-left{
	display: flex;
	align-items: center;
}

.template-bottom-right{
	display: flex;
	align-items: center;
}

.template-bottom-news{
	display: flex;
	margin-bottom: .14rem;
}

.template-bottom-news span:nth-child(2){
	margin: 0 0.1rem ;
}

.template-bottom-right .template-bottom-ad-del{
	margin-left: 0.34rem;
}

.template-bottom-tip{
	height: 0.3rem;
	font-size: 0.22rem;
	padding: 0 .13rem;
	box-sizing: border-box;
	border-radius: 0.02rem;
	border: 0.01rem solid #E0E0E0;
	display: flex;
	align-items: center;
	justify-content: center;

	text-align: center;
    line-height: 0.38rem;
    height: 0.38rem;
    max-width: 1.36rem;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.template-bottom-ad-del{
	width: 0.37rem;
	height: 0.24rem;
	background: #f8f8f8;
	border: 0.04rem;
	display: flex;
	justify-content: center;
	align-items: center;
}

.template-bottom-ad-del .iconfont{
	font-size: 0.16rem;
}

.template-bottom-houses-tip{
	display: flex;
	align-items: center;
	margin-right: 0.34rem;
	color: rgba(33, 33, 33, 1);
}

.template-bottom-houses-tip img{
	border-radius: 0;
	margin-right: 0.08rem;
}

.template-top-houses{
	font-size: 0.28rem;
}

.template-top-houses-name{
	font-size: 0.3rem;
	color: #212121;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.templ_1{
	display: flex;
	align-items: center;
	width: 80%;
}
.template-top-houses-name span{
	font-size: 0.22rem;
	/* color: #ADADAD; */
	margin-left: 0.2rem;
	padding: 0 0.1rem;
	border-radius: 2px;
	    background-color: #EFF2F3;
	    color: #61749E;
	/* border: 1px solid #E0E0E0; */
	box-sizing: content-box;

	text-align: center;
    line-height: 0.38rem;
    height: 0.38rem;
    max-width: 1.36rem;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.template-top-houses-name-tip{
	border: none !important;
	color: #fff !important;
	padding-top: 1px!important;
	padding-bottom: 1px!important;
	float: right;
}

.template-top-houses-price{
	display: flex;
	align-items: center;
	color: #757575;
	margin: 0.2rem 0;
}

.template-top-houses-price div{
	color: #FC4D39;
}

.template-top-houses-price div span{
	font-size: 0.34rem;
}

.template-top-houses-site{
	margin: 0 0.2rem 0 0.3rem;
	position: relative;
}

.template-top-houses-site:after{
	content: '';
	width: 0.01rem;
	height: 0.2rem;
	background-color: #757575;
	position: absolute;
	right: -17%;
	top: 53%;
	transform: translate(50%,-50%);
}

.template2{
	margin-bottom: 0.5rem;
}

.template2-video{
	/* width: 100vw; */
	height: 3.3rem;
	overflow: scroll hidden;
	-webkit-overflow-scrolling: touch;
	display: flex;
}

.template2-title{
	margin: 0.5rem 0 0.24rem 0.35rem;
}

.template2-video-item{
	width: 2.5rem;
	height: 3.3rem;
	margin-left: 0.35rem;
	/* background: green; */
	border-radius: 0.04rem;
	position: relative;
}

.template2-video-item .template2-video-bg{
	width: 2.5rem;
	height: 3.3rem;
	font-size: 0.26rem;
	text-align: center;
	line-height: 100px;
	overflow: hidden;
}
.template2-video-item .template2-video-bg img{
	width: 100%;
	height: 100%;;
}

/* 查看更多 */
.template2-video-place{
	width: 2.5rem;
	height: 3.3rem;
	font-size: 0.24rem;
}

.template2-video-place div{
	width: 2.5rem;
	height: 3.3rem;
	display: flex;
	justify-content: center;
	align-items: center;
}

.template2-video-place-box{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	padding: 0.2rem;
}

.template2-video-place img{
	width: 0.9rem;
	height: 0.9rem;
	margin-bottom: 0.2rem;
}

.template2-video-top{
	height: 0.42rem;
	position: absolute;
	top: 0.1rem;
	left: 0.1rem;
	font-size: 0.2rem;
	color: #fff;
	display: flex;
}

.template2-video-top span{
	height: 100%;
	display: flex;
	align-items: center;
	padding: 0 0.12rem;
}

.template2-video-top span:first-child{
	background: #285EE4;
	border-radius: 0.04rem 0px 0px 0.04rem;
}

.template2-video-top span:last-child{
	background: rgba(0, 0, 0, .5);
	border-radius: 0 0.04rem 0.04rem 0;
}

.template2-video-bottom{
	width: 100%;
	height: 0.5rem;
	font-size: 0.22rem;
	color: #FFFFFF;
	position: absolute;
	bottom: 0;
	display: flex;
	align-items: center;
	text-indent: 0.1rem;
	background: rgba(0, 0, 0, .5);
	border-radius: 0 0 0.04rem 0.04rem;
}

.template3-top{
	display: flex;
	/* height: 2.1rem; */
}

.template3-top img{
	width: 2.35rem;
	height: 1.78rem;
	object-fit: cover;
}

.template3-top-right{
	margin-left: 0.42rem;
	/* width: 60%; */
	width: calc(100% - 1.05rem);
	/* width: 60%; */
	flex: 1;
}

.template3-title{
	font-size: 0.34rem;
	display: flex;
	justify-content:space-between;
	/* align-items: center; */
	line-height: .4rem;
}

.template3-title span{
	color: #fff;
	font-size: 0.26rem;
	width: 0.7rem;
	height: 0.36rem;
	/* margin-right: 0.16rem; */
	/* background: linear-gradient(90deg, #6BCFFF 0%, #1EA5FE 100%); */
	border-radius: 0.02rem;
	display: flex;
	justify-content: center;
	align-items: center;
	line-height: 0.3rem;
}

.template3-title img{
	width: .94rem;
	height: .34rem;
	margin-right: .14rem;
}

.template3-tip{
	display: flex;
	margin: 0.16rem 0;
}

.template3-tip div{
	font-size: 0.22rem;
	color: rgba(173, 173, 173, 1);
	background-color: #EFF2F3;
	color:#61749E ;
	/* border: 0.01rem solid #E0E0E0; */
	border-radius: 0.02rem;
	padding: 0 0.14rem;
	display: flex;
	text-align: center;
	/* line-height: 0.38rem; */
	height: 0.3rem;
	max-width: 1.2rem;
	line-height: 0.3rem;
	overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1; /* 限制在一个块元素显示的文本的行数 */
    -webkit-box-orient: vertical; /* 垂直排列 */
    word-break: break-all;  /* 内容自动换行 */
}

.template3-tip div:nth-child(2){
	margin: 0 0.15rem;
}

.template3-site {
	font-size: 0.24rem;
	color: rgba(117, 117, 117, 1);
}

.template3-site .template-top-houses-site{
	margin-left: 0;
}

.template3-top-right .template-top-houses-price{
	font-size: 0.28rem
}

.template3-top-right .template-top-houses-price{
	margin: 0.1rem 0 0 0;
}

.template3-bottom{
	color: rgba(33, 33, 33, 1);
	font-size: 0.24rem;
	margin-top: 0.1rem;
	margin-left: 2.52rem;
	display: flex;
	align-items: center;
	position: relative;
	display: flex;
	flex-wrap: wrap;
}

.template3-bottom-tip{
	display: flex;
	align-items: center;
	margin-right: 0.3rem;
	margin-top: 0.1rem;
}

/* .template3-bottom-tip:nth-child(1){
	margin-top: 0;
} */

.template3-bottom-tip img{
	width: 0.3rem;
	height: 0.3rem;
	margin-right: 0.1rem;
	border-radius: 0.02rem;
}

.template3-bottom-vr span{
	height: 0.3rem;
	font-size: 0.22rem;
	color: #fff;
	background: linear-gradient(90deg, #FF813A 0%, #FE4533 100%);
	border-radius: 0.02rem;
	padding: 0 0.1rem;
}

.template3-bottom-apply{
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-top: .3rem;
}

.template3-bottom-apply .van-button{
	width: 1.49rem;
	height: .54rem;
	font-size: .26rem;
	padding: 0;
}

.template3-bottom-apply-box{
	width: 4.9rem;
	display: flex;
	flex-direction: column;
	
}

.template3-bottom-apply-info{
	color: rgba(252, 77, 57, 1);
	font-size: .26rem;
	margin-bottom: .1rem;
}

.template3-bottom-apply-info .iconfont{
	font-size: .26rem;
	margin-right: .14rem;
}

.template3-bottom-apply-people{
	font-size: .22rem;
	color: rgba(173, 173, 173, 1);
}

.template3-bottom-apply-people span{
	color: rgba(252, 77, 57, 1);
}

.template3-top-img-sale{
	position: relative;
}

.template3-top-img-sale::after{
	content: '津贴券';
	color: #fff;
	font-size: .2rem;
	width: .74rem;
	height: .3rem;
	border-radius: 0 0 .14rem 0;
	background: linear-gradient(90deg, #FCA36A 0%, #FC4D39 100%);
	display: flex;
	justify-content: center;
	align-items: center;
	position: absolute;
	left: 0;
	top: 0;
}