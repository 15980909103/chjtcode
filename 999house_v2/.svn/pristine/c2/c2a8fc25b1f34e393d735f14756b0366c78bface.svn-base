.van-dropdown-menu__bar{
	box-shadow: none;
}

.van-dropdown-item__content{
	max-height: none;
}

.van-dropdown-menu__title--active{
	font-size: .28rem;
	color: rgba(254, 130, 30, 1);
}

/* 更多 */
.drop-more{
	width: 100%;
	height: 7.16rem;
	position: relative;
}

.drop-more-item{
	padding: .3rem .34rem;
	box-sizing: border-box;
}

.drop-more-item:last-child{
	padding-bottom: 1.28rem;
}


.drop-more-title{
	margin-bottom: .24rem;
	font-size: .3rem;
}

.drop-more-box{
	display: flex;
	flex-wrap: wrap;
}

.drop-more-tip{
	width: 2.06rem;
	height: .64rem;
	background-color: rgba(250, 250, 250, 1);
	margin-bottom: .24rem;
	line-height: .64rem;
	text-align: center;
}

.drop-more-tip:nth-child(3n-1){
	margin: 0 .32rem;
}

.drop-more-tip-active{
	background-color: rgba(254, 130, 30, 1);
	color: #fff;
}

.drop-more-wrap{
	height: 6.66rem;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

.drop-btn{
	width: 100%;
	height: 1.28rem;
	position: absolute;
	bottom: 0;
	left: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	background-color: #fff;
	border-top: 1px solid rgba(240, 240, 240, 1);
}

.drop-del{
	margin-right: .26rem;
}

.drop-btn-item{
	width: 3.28rem;
	height: .88rem;
	font-size: .3rem;
}

.drop-price{
	width: 100%;
	height: 7.16rem;
	position: relative;
}

.drop-price-box{
	width: 100%;
	height: 5.3rem;
	display: flex;
}

.drop-price-left{
	width: 3.43rem;
	border-right: 1px solid rgba(240, 240, 240, 1);
}

.drop-price-right{
	width: 4.06rem;
}

.drop-price-left,
.drop-price-right{
	height: 4.95rem;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

.drop-price-right span{
	width: 100%;
	height: 100%;
	text-indent: none;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

.drop-price-item{
	width: 100%;
	height: 1.04rem;
	/* border-bottom: 0.01rem solid rgba(240, 240, 240, 1); */
	box-sizing: border-box;
	padding-left: .4rem;
	line-height: 1.04rem;
	font-size: .32rem;
}

.drop-price-item-active{
	color: rgba(254, 130, 30, 1);
}

.drop-price-input{
	width: 100%;
	height: 2.2rem;
	position: absolute;
	bottom: 0;
	border-top: 1px solid rgba(240, 240, 240, 1);
	box-sizing: border-box;
	padding: .22rem .32rem .28rem;
}

.drop-price-input-title{
	font-size: .32rem;
}

.drop-price-input-price{
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.drop-price-input-wrap{
	display: flex;
	justify-content: space-between;
	margin-top: .22rem;
}

.drop-price-input-box{
	display: flex;
	align-items: center;
}

.drop-price-input-box .van-cell{
	width: 1.58rem;
	height: .64rem;
	padding: 0;
	border: 1px solid #E0E0E0;
}

.drop-price-input-box .van-field__control{
	height: .64rem;
	text-align: center;
}

.drop-price-input-interval{
	height: .02rem;
	width: .3rem;
	background-color: rgba(117, 117, 117, 1);
	margin: 0 .12rem;
}

.drop-price-btn{
	width: 2.36rem;
	height: .88rem;
}

.drop-type{
	width: 100%;
	height: 7.16rem;
	position: relative;
	box-sizing: border-box;
	padding: 0 .32rem;
}

.drop-type-wrap{
	height: 5.88rem;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
}

.drp-type-box{
	width: 100%;
	height: 1.06rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: .32rem;
	border-bottom: 0.01rem solid rgba(240, 240, 240, 1);
}

.drp-type-box .iconnewxuanzhongduoxuan{
	color: rgba(254, 130, 30, 1);
}

.text-none{
	
}