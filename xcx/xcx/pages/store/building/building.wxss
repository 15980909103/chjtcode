#my-loupan{width: 100vw;height: 86vh;background-color: #FFF;}
.my-lp-lists{width: 90vw;padding: 0 5vw 3vh;}
.my-lp-list{width: 90vw;height: 12vh;border-bottom: 1px solid #eee;padding: 2.5vh 0;display: flex;justify-content:space-between;}
.my-lpimg{width: 25vw;position: relative;}
.my-lpimg .my-lp-ig{width:25vw;height: 12vh;border-radius: .8vw;}
.my-lpimg .my-huo{position: absolute;left: 1.5vw;bottom: .6vh; color:#FFF;font-size: 2.8vw;display: flex;align-items:flex-end;}
.my-lpimg .my-huo image{width: 5vw;height: 5vw;}
.my-lp-center{width: 62vw;display: flex;flex-direction:column;align-content:space-between;}
.my-lp-c1{font-size:2.5vh;font-weight:600;line-height: 4vh;display: flex;align-items:center;}
.my-lp-c2{color:#999;font-size:2vh;line-height: 4vh;}
.my-lp-c3{color:#d12527;font-size:2.5vh;font-weight:600;line-height: 4vh;}
.my-lp-c1 .my-c-1-1{max-width: 52vw;}
.my-lp-c1 .my-c-1-2{width: 10vw;height: 3vh;line-height: 3vh;margin-left:2vw;background-color: #d12527;color: #FFF;font-size: 3vw;font-weight: 500;text-align: center;border-radius: 1vw;}
#my-search{width: 100vw;height: 8vh;display: flex;align-items:center;}
.field-index--van-field{background-color: #eee !important;}
.my-op1{width: 20vw;margin:1vh 0 1vh 5vw;line-height: 3.5vh;}
.my-font2{font-size: 4.5vw;}
.my-font3{font-size: 3.5vw;}
.my-font4{font-size: 2.6vw;}
.van-search{width: 80vw;}
.triangle-down{position: relative;color:#000;}
.triangle-up{position: relative;color:#D12526;}
.triangle-down::after{content:"\20";width: 0;border-left: 1.5vw solid transparent;border-right: 1.5vw solid transparent;border-top: 2vw solid #000;position: absolute;top: 1vh;right: 1vw;}
.triangle-up::after{content:"\20";width: 0;border-left: 1.5vw solid transparent;border-right: 1.5vw solid transparent;border-bottom: 2vw solid #D12526;position: absolute;top: 1vh;right: 1vw;}
.sort-down::after{content:"\20";width: 6vw;height: 4vh;position:absolute;left: 2vw;background-size: 100% 100%;background-image: url(data:image/png;base64,)}
.sort-up::after{content:"\20";width: 6vw;height: 4vh;position:absolute;left: 2vw;background-size: 100% 100%;background-image: url(data:image/png;base64,)}
#my-sort{width: 100vw;height: 6vh;border-top: 1px solid #eee;border-bottom: 1px solid #eee;display: flex;justify-content:space-between;align-items:center;}
.sort-li{width: 21vw;text-align: center;font-size: 4vw;}
.sort-img{width: 14vw;height: 4vh;position: relative;}
.sort-1{height: 4vh;border-right: 1px solid #eee;}
#my-sort-detail{width:100vw;height: 86vh;position: absolute;z-index: 5;background-color:#f5f5f5;}
.my-animation{top: -200vh;transition:top .8s ease;}
.is-showdetail{top:14vh}
/* 栏目数据演示 */
.my-tick{color: #D12526;}
.my-tick2{color: #FFF !important;background-color: #F44E50 !important;}
.my-sort-d0{width:100vw;height: 86vh;background-color: #FFF;}
.my-sort-d1{text-align: center;font-size: 4vw;padding: 1.8vh 0;border-bottom: 1px solid #eee;color: #6c6c6c;background-color: #FFF;}
.my-sort-d2{width: 100vw;display: flex;}
.my-sort-d2 .my-so-2-1{width: 27vw;background-color: #FFF;padding-left: 5vw;}
.my-sort-d2 .my-so-2-2{width: 68vw;background-color: #FAFAFA;color: #6c6c6c;}
.my-sort-d2 .my-so-2-li{height: 5vh;padding: .8vh 5vw;font-size: 4vw;line-height: 5vh;border-bottom: 1px solid #eee;}
.my-sort-d2 .my-so-2-1 .my-so-2-li{padding-left: 0;}
.my-sort-dd4{width: 100vw;min-height: 86vh;background-color: #FFF;padding-bottom: 10vh;}
.my-sort-d4{width: 100vw;}
.sort4-title{width: 90vw;font-size: 4.5vw;font-weight: 600;padding: 2vh 5vw .5vh;}
.sort4-contents{width: 85vw;padding: 0 7.5vw;display: flex;flex-wrap:wrap;justify-content:flex-start;}
.sort4-li{width: 25vw;background-color: #eee;color:#999;text-align: center;padding: 1vh 0;margin-top: 2vh;font-size: 3.3vw;}
.sort4-li-b{margin-left: 5vw;margin-right: 5vw;}
.my-sort4-footer{width: 90vw;height: 6vh;padding:2vh 5vw;position: fixed;bottom: 0;display: flex;background-color: #FFF;}
.sort4-btn1{width: 45vw;background-color:#DDDDDD;color: #5A5A5A;text-align: center;line-height: 6vh;font-size: 4.3vw;}
.sort4-btn2{width: 45vw;background-color: #E03C32;color: #FFF;text-align: center;line-height: 6vh;font-size: 4.3vw;}

.city {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.current-query {
  padding: 10rpx 0;
  width: 100vw;
  background-color: #fff;
  border-bottom: 16rpx solid #f7f7f7;
}

.current-query__list {
  display: flex;
  align-content: center;
}

.current-query__list-item {
  margin-left: 24rpx;
  padding: 0 12rpx;
  font-size: 24rpx;
  white-space: nowrap;
  border-radius: 4rpx;
  background-color: #f1f1f1;
}
