				<div class="check_list">
					<van-checkbox-group v-model="result" checked-color="#FE821E">
						<van-checkbox name="a">
							<img src="../../static/image/attache_active.png" />
							<p>测试人员</p>
						</van-checkbox>
						<van-checkbox name="a1">
							<img src="../../static/image/attache_active.png" />
							<p>测试人员1</p>
