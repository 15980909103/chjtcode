.my-content{width: 100vw;min-height: 100vh;background-color: #FFF;}
[v-cloak] {display: none;}
#my-feedback{width: 100vw;padding: 2vh 5vw;}
.my-title{font-size: 4.2vw;}
.my-btn-li{padding: 2vh 0; display: flex;justify-content:space-between;border-bottom: 1px solid #f5f5f5;}
.my-btn{font-size: 3.5vw;background-color: #F7F7F7;color: #727272;text-align: center;padding: 1vh 3vw;}
.my-tick{color: #FFF;background-color: #F44E50;}
.my-title2{font-size: 4.2vw;padding: 2vh 0;}
.my-ta-content{width: 90vw;position: relative;}
.my-textarea{width: 90vw;height: 24vh;background-color: #F7F7F7;font-size: 4vw;padding: 2vh 5vw 6vh;border: 0;}
.my-num{position: absolute;right:5vw;bottom: 2vh;font-size: 3.3vw;color: #808080;}
.my-btn-success{width: 90vw;height: 8vh;line-height:8vh;background-color: #DD2D30;margin-top: 3vh;color: #FFF;text-align: center;border-radius: 1.5vw;}