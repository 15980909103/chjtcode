.tabs{
	
}
.tabs .tab_list{
	display: flex;
	justify-content: space-around;
	padding: 0.26rem 0;
}
.tab_item{
	color: #333333;
	text-align: center;
	font-size: 0.3rem;
}
.tab_item img{
	width: .38rem;
	height: auto;
	margin: 0 auto .14rem;
	display: block;
}
.tab_item.active{
	color: #D22A2C;
}
.tabs_screen{
	
}
.screen_list{
	display: flex;
	justify-content: space-around;
	line-height: .85rem;
	border-top: 1px solid #E0E0E0;
}
.screen_item{
	color: #666666;
	text-align: center;
	font-size: .28rem;
}
.scroll-content{
	width: 100vw;
	min-height:10vh;
	overflow: hidden;
	/* padding-top: 0.28rem; */
	position: relative !important;
	background-color: #F7F7F7;
}
.input-row{
	position: relative;
	display: block;
	margin: .28rem;
	padding: 0 .23rem;
	background-color: #fff;
}
.profile-card {
	/* display: block;
  margin: .28rem;
  padding: 0 .23rem;
  background-color: #fff; */
}
.failure {
  filter: grayscale(100%);
}

.profile-card__header,
.profile-card__approve,
.profile-card__body {
  width: 100%;
  display: flex;
  align-items: center;
}

.profile-card__header {
	line-height: 0.93rem;
  /* padding: .625rem 0; */
  border-bottom: 1px solid #F7F7F7;
  font-size: 0.3rem;
  color: #333333;
}

.profile-card__header-avatar {
  margin-right: .14rem;
  width: 0.46rem;
  height: 0.46rem;
  border-radius: 50%;
  overflow: hidden;
  object-fit: cover;
}
.profile-card__approve{
	line-height: 0.93rem;
	border-bottom: 1px solid #EBEBEB;
}
.profile-card__approve-profile {
  display: flex;
  align-items: center;
  max-width: 45%;
  overflow: hidden;
}
.profile-card__approve-profile .avatar{
	margin-right: .14rem;
	/* margin-bottom: .3rem; */
	position: relative;
	display: block;
	flex-shrink: 0;
	font-size: 0.3rem;
	color: #333333;
	width: .46rem;
	height: .46rem;
	border-radius: 500rem;
	border: 1px solid #FBE2E3;
	background-color: #fff;
	overflow: hidden;
}
.profile-card__approve-profile .avatar img{
	width: .46rem;
	height: .46rem;
	object-fit: cover;
	
}
.profile-card__approve-profile .name {
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 0.3rem;
  color: #333333;
}
.profile-card__body {
  position: relative;
  padding: .24rem 0;
  align-items: normal;
}
.profile-card__body-cover {
  /* position: relative; */
  align-self: flex-start;
  /* width: 9.3rem;
  height: 6.25rem; */
  width: 21.54%;
  /* border-radius: .25rem; */
  overflow: hidden;
}
.profile-card__body-cover img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.profile-card__body-info {
  position: relative;
  margin-left: .22rem;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
}
.profile-card__body-info .info-line {
  margin-bottom: 0;
  font-size: 0.22rem;
  color: #999999;
}

.info-line em {
  font-size: .26rem;
  font-style:normal
}
.info-line.price em {
	font-style:normal
  float: left;
  margin-right: .22rem;
}

.info-line.name {
  color: #000;
}

.info-line.price {
  color: #d12527;
}
.info-line.price span {
  /* float: left; */
  color: #8f8f94;
}

/* status */
.info-line.status span {
  padding: 1px .375rem;
  border-radius: .25rem;
  color: #fff;
}
.info-line .doing {
  background-color: #D12527;
}
.info-line .watting {
  background-color: #364FD6;
}
.info-line .yellow {
  color: #E69A1E !important;
  background-color: #FFF7E9;
}
.info-line .blue {
  color: #4291DF !important;
  background-color: #E6F6FF;
}
.info-line .purple {
  color: #7C42E0 !important;
  background-color: #F3ECFF;
}
.info-line .red {
  color: #E65835 !important;
  background-color: #FFEFEA;
}
.profile-card__body .failure-icon {
  position: absolute;
  top: 0;
  right: 1.68rem;
  width: 1.29rem;
}

.profile-card__body-status {
  position: relative;
  /* margin-left: 1rem; */
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.profile-card__body-status-tip {
  padding: 0 .15rem;
  display: inline-block;
  height: .3rem;
  font-size: .22rem; 
  line-height: .3rem;
  color: transparent;
  transform: skew(-15deg, 0deg);
}

.profile-card__body-status-tip:before {
  content: attr(data-label);
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  transform: translate(-50%, -50%) skew(15deg, 0deg);
 font-size: .22rem;
 line-height: .3rem;
  text-align: center;
  color: #fff;
}
.profile-card__body-timer {
  margin-top: .13rem;
  font-size: .22rem;
  color: #999999;
}
.profile-card__body-status-tip.blue {
  background-color: #6CACFD;
}

.profile-card__body-status-tip.red {
  background-color: #FC4343;
}

.profile-card__body-status-tip.orange {
  background-color: #F37B56;
}

.profile-card__body-status-tip.gray {
  background-color: #E6E6E6;
}
.profile-card__tail {
	line-height: .73rem;
	border-top: 1px solid #EBEBEB;
}
.profile-card__tail p{
	color: #666666;
	font-size: .24rem;
}
.profile-card__tail p span{
	color: #D22A2C;
}
.commit-profile {
  position: fixed;
  bottom: 20vh;
  right: .4rem;
  z-index: 20;
  width: .97rem;
  height: .97rem;
  
}
.commit-profile img{
  width: 100%;
  height: auto;
}
