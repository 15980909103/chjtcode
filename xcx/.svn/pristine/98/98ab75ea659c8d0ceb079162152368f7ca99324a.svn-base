.ccl-0-1 div {
  overflow: hidden;
  text-overflow: ellipsis;
}

.ccl-0-1 div, .ccl-0-2 > div {
  white-space: nowrap;
}

:root {
  font-size: 16px;
}

.search {
  padding: 0.5rem 0.75rem;;
  background-color: #fff;
  box-sizing: border-box;
}
.search input {
  margin-bottom: 0;
}
.search .mui-placeholder {
  background-color: #F7F7F7 !important;
}

.management {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100vw;
  padding: 0.5rem 0.75rem;
  border-top:  .625rem solid #F7F7F7;
  border-bottom:  .625rem solid #F7F7F7;
}
.management .icon {
  width: 1.875rem;
}
.management .label {
  margin-left: .75rem;
}
.management .arrow {
  flex: 1;
}
.management .arrow::before {
  content: '';
  display: block;
  float: right;
  width: .5rem;
  height: .5rem;
  border-right: 1px solid #999;
  border-bottom: 1px solid #999;
  transform: rotate(-45deg);
}


.swiperTitle .swiper-slide {
  height: 3rem;
  line-height: 3rem;
  font-size: .875rem;
}

.swiperTitle .swiper-slide.active {
  color: #FC4343;
}

.sp0-content {
  padding: .5rem .875rem 0;
}
.sp0-content .sp0-nav, .sp0-content .sp0-con,
.currentchild1, .currentchild1 .cc1-2 {
  width: 100%;
}

.currentchild1 .cc1-2 .my-lpimg,
.currentchild1 .cc1-2 .my-lpimg img {
  width: 23.46vw;
  height: 16.8vw;
  object-fit: cover;
}
.my-lp-c2 {
  font-size: .75rem;
}
.my-lp-c4 div {
  padding: 0 .375rem;
  font-size: .75rem;
  line-height: 1rem;
}


.profile-card {
  margin: 1rem 1rem 0;
  padding: 0 .75rem;
  width: calc(100vw - 2rem);
  background-color: #fff;
}
.profile-card.failure {
  filter: grayscale(100%);
}

.profile-card__header,
.profile-card__approve,
.profile-card__body {
  width: 100%;
  display: flex;
  align-items: center;
}

.profile-card__header {
  padding: .625rem 0;
  border-bottom: 1px solid #F7F7F7;
}

.profile-card__header-avatar {
  margin-right: .75rem;
  width: 1.875rem;
  height: 1.875rem;
  border-radius: 50%;
  overflow: hidden;
  object-fit: cover;
}

.profile-card__header-nikename {
  font-size: 1rem;
}

.profile-card__approve-label {
  font-size: 1rem;
  color: #2895DA;
}

.profile-card__approve-label.font-red-color {
  color: #CE2728;
}

.profile-card__header-timer {
  flex: 1;
  text-align: right;
  font-size: .75rem;
  color: #B7B7B7;
}



.profile-card__approve {
  padding: .5rem 0px;
  justify-content: space-between;
  border-bottom: 1px solid #F7F7F7;
}

.profile-card__approve-profile {
  display: flex;
  align-items: center;
  max-width: 45%;
  overflow: hidden;
}

.profile-card__approve-profile .avatar {
  margin-right: .5rem;
  margin-bottom: .3rem;
  position: relative;
  display: block;
  flex-shrink: 0;
}

.profile-card__approve-profile .avatar img {
  width: 2.375rem;
  height: 2.375rem;
  border-radius: 50%;
  border: 1px solid #FBE2E3;
  object-fit: cover;
  overflow: hidden;
  background-color: #fff;
}

.profile-card__approve-profile .avatar .type {
  padding: 0 .5rem;
  position: absolute;
  left: 50%;
  bottom: 0;
  font-size: .625rem;
  white-space: nowrap;
  min-width: 100%;
  height: 1rem;
  line-height: 1rem;
  text-align: center;
  color: #fff;
  border-radius: .625rem;
  background-color: rgba(0,0,0, .6);
  overflow: hidden;
  transform: translate(-50%, 30%) scale(.7);
}

.profile-card__approve-profile .name {
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}


.profile-card__body {
  position: relative;
  padding: .75rem 0;
  align-items: normal;
}

.profile-card__body-cover {
  position: relative;
  align-self: flex-start;
  width: 21.54%;
  border-radius: .25rem;
  overflow: hidden;
}
.profile-card__body-cover:before {
  content: '';
  display: block;
  padding-top: 97.99%;
  width: 100%;
}
.profile-card__body-cover img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.profile-card__body-info {
  position: relative;
  margin-left: .75rem;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
}
.profile-card__body-info .info-line {
  margin-bottom: 0;
  font-size: .625rem;
}

.info-line em {
  font-size: .875rem;
}
.info-line.price em {
  float: left;
  margin-right: .5rem;
}

.info-line.name {
  color: #000;
}

.info-line.price {
  color: #d12527;
}
.info-line.price span {
  float: left;
  color: #8f8f94;
}

/* status */
.info-line.status span {
  padding: 1px .375rem;
  border-radius: .25rem;
  color: #fff;
}
.info-line .doing {
  background-color: #D12527;
}
.info-line .watting {
  background-color: #364FD6;
}
.info-line .yellow {
  color: #E69A1E !important;
  background-color: #FFF7E9;
}
.info-line .blue {
  color: #4291DF !important;
  background-color: #E6F6FF;
}
.info-line .purple {
  color: #7C42E0 !important;
  background-color: #F3ECFF;
}
.info-line .red {
  color: #E65835 !important;
  background-color: #FFEFEA;
}
.profile-card__body .failure-icon {
  position: absolute;
  top: 0;
  right: 5rem;
  width: 15.46vw;
  transform: translate(0, -50%);
}

.profile-card__body-status {
  position: relative;
  margin-left: 1rem;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.profile-card__body-status-tip {
  padding: 0 .25rem;
  display: inline-block;
  height: 1rem;
  font-size: .75rem;
  line-height: 1rem;
  color: transparent;
  transform: skew(-15deg, 0deg);
}

.profile-card__body-status-tip:before {
  content: attr(data-label);
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  transform: translate(-50%, -50%) skew(15deg, 0deg);
  font-size: .75rem;
  line-height: 1rem;
  text-align: center;
  color: #fff;
}

.profile-card__body-status-tip.blue {
  background-color: #6CACFD;
}

.profile-card__body-status-tip.red {
  background-color: #FC4343;
}

.profile-card__body-status-tip.orange {
  background-color: #F37B56;
}

.profile-card__body-status-tip.gray {
  background-color: #E6E6E6;
}

.swiperTitle {
  position: relative;
}

.swiperTitle, .filter {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.filter {
  flex-shrink: 0;
  padding: 0 1rem;
}
.filter-button {
  width: 1rem;
  flex-shrink: 0;
}
.filter-button:not(:last-child) {
  margin-right: 1rem;
}

#my-div2 {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100vw;
  transform: translate(0, 100%);
  background-color: #fff;
  z-index: 10;
}

.commit-profile {
  position: fixed;
  bottom: 20vh;
  right: 1rem;
  z-index: 20;
  width: 3rem;
  height: 3rem;
}
