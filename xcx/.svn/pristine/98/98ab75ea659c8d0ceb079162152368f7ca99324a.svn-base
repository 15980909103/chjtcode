.my-content{width: 100vw;min-height: 100vh;background-color: #FFF;overflow: hidden;}
[v-cloak] {display: none;}
#my-div1{width: 100vw;height: 12vh;padding: 1vh 5vw;display:flex;justify-content:space-between;align-items:center;border-bottom: 1px solid #F1F1F1;}
.div-1{width: 20vw;}
.div-1-1{width: 10vw;height: 10vw;margin: 0 auto;}
.div-1-2{text-align: center;font-size: 4vw;}
.div-1:nth-child(2){
