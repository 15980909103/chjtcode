@font-face {
    font-family: "aui_iconfont";
    src: url('aui-iconfont.ttf') format('truetype');
}
.my-content{width: 100vw;min-height: 100vh;background-color: #FFF;}
[v-cloak] {
    display: none !important;
}
#my-footer{width: 100vw;height: 10vh;padding: 1.5vh 5vw;background-color: #FFF;position: fixed;bottom: 0;z-index: 100;border-top: 1px solid #eee;display: flex;justify-content:space-between;align-items:center;}
.footer-li{display: flex;flex-direction:column;justify-content:center;align-items:center;width: 6vh;font-size:17px;}
.footer-li img{width: 4.5vh;height: 4.5vh;}
.footer-li div{font-size: .85em;color: #999;}
.mui-focusin #my-footer{display:none !important;}
.my-tick div{color: #D74546 !important;}
input[type=search]{height: 5vh !important;margin-bottom:0 !important;}
.mui-input-row.mui-search.mui-active::before{line-height: 5vh !important;}
#my-scroll{
    position: relative;
    width: 100vw;
    height: calc(100vh - 5rem);
    box-sizing: border-box;
    background-color: #F7F8FA;
}

.my-loupan{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    /* width: 40vw; */
    height: 2.5rem;
    background-color: #FFF;
    padding: 1vh 4vw;
    font-size: 4vw;
    display: flex;
    justify-content:space-between;
    align-items:center;
    z-index: 10;
    border-bottom: 1px solid #eee;
}
.my-loupan img{
    width: 7vw;
    height: 7vw;
}
.my-loupan div{
    width: 80%;
    text-align: left;
}
.my-lp-lists{
    margin-top: 1vh;
    width: 100vw;
}
.my-lp-list{
    width: 100vw;
    min-height: 12vh;
    border-bottom: 1px solid #eee;
    padding: 1rem;
    background-color: #FFF;
    display: flex;
    align-items: center;
    /* justify-content:space-between; */
}
.my-lpimg{
    width: 6.5rem;
    height: 5rem;
    /* width: 25vw; */
    position: relative;
    overflow: hidden;
}
.my-lpimg .my-lp-ig{
    /* width:30vw; */
    /* width: 100%; */
    /* height: auto; */
    height: 5rem;
    width: auto;
    min-width: 100%;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    /* min-height:3.5rem ; */
    /* height: 20vw; */
    /* height: 3.5rem; */
    border-radius: .8vw;
}
.my-lpimg .my-huo{
    position: absolute;
    left: 1.5vw;
    bottom:0rem;
    color:#FFF;
    font-size: 2.8vw;
    /* display: flex;
    align-items:center; */
}
.my-lpimg .my-huo img{
    width: 5vw;
    height: 5vw;
    display: inline-block;
    vertical-align: top;
}
.my-lpimg .my-huo div{
    display: inline-block;
    vertical-align: text-bottom;
}
.my-lp-center{
    width: 42vw;
    display: flex;
    flex-direction:column;
    align-content:space-between;
    margin-left: 0.75rem;
}
.my-btn{
    width: 15vw;
    display: flex;
    align-items:center;
    flex-direction:column;
    justify-content:center;

}
.my-btn div{
    color: #FFF;
    width: 3.2rem;
    height: 1.4rem;
    font-size: 0.65rem;
    text-align: center;
    border-radius: 5px;
    line-height: 1.4rem;
}
.my-btn .kt{
    background: linear-gradient(45deg,#D22527,#EA5101);
}
.my-btn .ykt{
    background: #AAAAAA;
}
.my-lp-c1{font-size:2.5vh;font-weight:600;line-height: 3vh;display: flex;align-items:center;}
.my-lp-c2{color:#999;font-size:2vh;line-height: 3vh;}
.my-lp-c3{color:#d12527;font-size:2.4vh;font-weight:600;}
.my-lp-c3 span{font-size: 3vw;font-weight: 500;color: #999;}
.my-lp-c1 .my-c-1-1{max-width: 62vw;}
.my-lp-c1 .my-c-1-2{width: 10vw;height: 3vh;line-height: 3vh;margin-left:2vw;background-color: #d12527;color: #FFF;font-size: 3vw;font-weight: 500;text-align: center;border-radius: 1vw;}
.my-lp-c4{display: flex;flex-wrap:wrap;}
.my-lp-c4 div{
    /* margin-right: 3vw; */
    /* font-size: 3vw; */
    /* padding:.3vh 2.5vw; */
    font-size: 0.65rem;
    font-weight: 500;
    text-align: center;
    border-radius: 2px;
    padding: 2px 5px;
    box-sizing: content-box;
    height: 0.9rem;
    line-height: 0.9rem;
    margin-right: 0.5rem;
    margin-top:.4vh;
}
.my-lp-c4 div:last-child{
    margin-right: 0;
}
.my-style1{background-color: #d12527;color: #FFF;}
.my-style2{background-color: #FFF7EA;color: #E8A943;}
.my-style3{background-color: #E5F6FE;color: #589EE5;}
.my-style4{background-color: #F3ECFE;color: #8046E1;}
.my-style5{background-color: #FEEFEA;color: #E76342;}
/*===========================*/
#my-search{
    position: relative;
    width: 100%;
    height: 3rem;
    display: flex;
    align-items:center;
    width: 100%;
    z-index: 10;
    background: #ffffff;
}
#my-search .mui-input-clear{
    background: #f1f1f1;
}
.field-index--van-field{background-color: #eee !important;}
.my-op1{
    margin:1vh 0 1vh 0vw;
    padding-right: 1rem;
    width: 20vw;
    line-height: 3.5vh;
    text-align: right;
}
.my-font2{font-size: 4.5vw;}
.my-font3{font-size: 3.5vw;}
.my-font4{font-size: 2.6vw;width: 18vw !important;}
.van-search{width: 80vw;}
.triangle-down{position: relative;color:#000;}
.triangle-up{position: relative;color:#D12526;}
.triangle-down::after{
    content:"\20";
    width: 0;
    border-left: 0.3rem solid transparent;
    border-right: 0.3rem solid transparent;
    border-top: 0.4rem solid #000;
    position: absolute;
    top: 0.5rem;
    right: 0.4rem;
}
.triangle-up::after{content:"\20";width: 0;border-left: 1.5vw solid transparent;border-right: 1.5vw solid transparent;border-bottom: 2vw solid #D12526;position: absolute;top: 1vh;right: 1vw;}
.sort-down::after{
    content:"\20";
    width: 1.6rem;
    height: 1.6rem;
    position:absolute;
    left: 50%;
    top: 50%;
    margin-top: -0.8rem;
    margin-left: -0.8rem;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,)
}
.sort-up::after{
    content:"\20";
    width: 1.6rem;
    height: 1.6rem;
    position:absolute;
    left: 50%;
    top: 50%;
    margin-top: -0.8rem;
    margin-left: -0.8rem;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,)
}
#my-sort{
    position: relative;
    width: 100%;
    z-index: 10;
    height: 2rem;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
    display: flex;
    justify-content:space-between;
    align-items:center;
    background: #ffffff;
}
.aui-iconfont{
    position: relative;
    font-family: "aui_iconfont" !important;
    font-size: 0.7rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.aui-icon-right:before {
    content: "\e6f5";
}
.sort-li{
    width: 30%;
    text-align: center;
    font-size: 4vw;
}
.sort-img{
    width: 14%;
    height: 2rem;
    /* height: 4vh; */
    position: relative;
}
.sort-1{height: 4vh;border-right: 1px solid #eee;}
#my-sort-detail{width:100vw;height: 86vh;position: absolute;z-index:101;background-color:#f5f5f5;}
.my-animation{top: -200vh;transition:top .8s ease;}
.is-showdetail{top: 5rem}
.my-animation2{bottom: -200vh;transition:bottom .8s ease;}
.is-showdetail2{bottom:0}
/* 栏目数据演示 */
.my-tick{color: #D12526;}
.my-tick2{color: #FFF !important;background-color: #F44E50 !important;}
.my-sort-d0{width:100vw;background-color: #FFF;padding: 0 5vw;}
.my-sort-d1{text-align: center;font-size: 4vw;padding: 1.8vh 0;border-bottom: 1px solid #eee;color: #6c6c6c;background-color: #FFF;}
.my-sort-d2{width: 100vw;display: flex;}
.my-sort-d2 .my-so-2-1{width: 32vw;background-color: #FFF;padding-left: 5vw;}
.my-sort-d2 .my-so-2-2{width: 78vw;background-color: #FAFAFA;color: #6c6c6c;}
.my-sort-d2 .my-so-2-li{height: 6.6vh;padding: .8vh 5vw;font-size: 4vw;line-height: 5vh;border-bottom: 1px solid #eee;}
.my-sort-d2 .my-so-2-1 .my-so-2-li{padding-left: 0;}
.my-sort-dd4{width: 100vw;min-height: 86vh;background-color: #FFF;padding-bottom: 10vh;}
.my-sort-d4{width: 100vw;}
.sort4-title{width: 100vw;font-size: 4.5vw;font-weight: 600;padding: 2vh 5vw .5vh;}
.sort4-contents{width: 100vw;padding: 0 7.5vw;display: flex;flex-wrap:wrap;justify-content:flex-start;}
.sort4-li{width: 25vw;background-color: #eee;color:#999;text-align: center;padding: 1vh 0;margin-top: 2vh;font-size: 3.3vw;}
.sort4-li-b{margin-left: 5vw;margin-right: 5vw;}
.my-sort4-footer{width: 100vw;height: 10vh;padding:2vh 5vw;position: fixed;display: flex;background-color: #FFF;z-index: 102;}
.sort4-btn1{width: 45vw;background-color:#DDDDDD;color: #5A5A5A;text-align: center;line-height: 6vh;font-size: 4.3vw;}
.sort4-btn2{width: 45vw;background-color: #E03C32;color: #FFF;text-align: center;line-height: 6vh;font-size: 4.3vw;}

.city-title{color: #999;font-size: 5vw;font-weight: 600;padding:1.5vh 0;}
.city-hot-div{display:flex;flex-wrap:wrap;padding:2vw;}
.city-hot-li{padding:1vh 5vw;border:0 #ccc solid;border-radius:1.5vw;margin:1.5vw;background-color:#eee;width: 18vw;text-align: center;}
.city-li{font-size: 4vw;padding: .8vh 0;border-bottom: 1px solid #eee;}


.mult-select__list {
    position: fixed;
    top: 5rem;
    left: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 100%;
    height: 2rem;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
    background-color: #fff;
    z-index: 10;
    overflow: hidden;
    overflow-x: scroll;
    overflow-y: hidden;
}

.mult-select__list-options {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.mult-select__list-options-item {
    margin-left: .6em;
    padding: 0 .4rem;
    font-size: .75rem;
    border-radius: 4px;
    color: #333;
    white-space: nowrap;
    background-color: #eee;
}

#my-scroll.mult-padding {
    padding-top: 7rem;
}


.focus-building__button {
    width: 6em;
    height: 3em;
    line-height: 3em;
    font-size: .75rem;
    color: #fff;
    text-align: center;
    flex-shrink: 0;
    background-color: #aaa;
    -webkit-box-shadow: 0px .25rem 1rem .25rem rgba(119, 119, 119, .18);
    box-shadow: 0px .25rem 1rem .25rem rgba(119, 119, 119, .18);
    border-radius: .5rem;
}

.focus-building__button.bright {
    -webkit-box-shadow: 0px .25rem 1rem .25rem rgba(161, 4, 6, .18);
    box-shadow: 0px .25rem 1rem .25rem rgba(161, 4, 6, .18);
    background: linear-gradient(30deg,rgba(209,37,39,1),rgba(235,82,0,1));
}

.my-lpimg, .my-lpimg .my-lp-ig {
    width: 26.66vw;
    object-fit: cover;
}

.my-lp-center {
    margin-left: .75rem;
    margin-right: 1rem;
    flex: 1;
    overflow: hidden;
}

.my-lp-center > div:not(.my-lp-c4) {
    display: inline-block;
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.my-lp-c1 {
    font-size: 1rem;
    line-height: 1em;
}

.my-lp-c2 {
    font-size: .75rem;
}

.my-lp-c3 {
    font-size: 1.125rem;
}


.my-lp-list .select input {
    width: 1rem;
    height: 1rem;
}

.commit-selected {
    position: fixed;
    padding: 0;
    left: 0;
    bottom: 0;
    width: 100vw;
    line-height: 3rem;
    z-index: 20;
    box-sizing: border-box;
}

.fixed-page {
    height: 100vh;
    overflow: hidden;
}
