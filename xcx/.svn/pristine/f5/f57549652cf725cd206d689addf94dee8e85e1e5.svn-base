.mui-pull-bottom-tips{display: none;}

.record-follow {
  position: relative;
  display: flex;
  flex-direction: column;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background-color: #fff;
}

.profile {
  padding: .875rem 1rem 1rem;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100vw;
  background-color: #fff;
  border-top: .35rem solid #F7F7F7;
  border-bottom: 1px solid #F7F7F7;
}

.profile-avatar {
  width: 14.14vw;
  height: 14.14vw;
  max-width: 106px;
  max-height: 106px;
  border: .125rem solid #f7f7f7;
  border-radius: 50%;
  overflow: hidden;
}

.profile-avatar img {
  width: 100%;
  height: 100%;
}

.profile-info {
  margin-left: 1rem;
  display: flex;
  flex-direction: column;
  flex: 1;
}

.profile-info .line {
  margin-bottom: 0;
}

.profile-info__name {
  display: flex;
  align-items: center;
}
.profile-info__name .name {
  margin-right: .5rem;
  font-size: 1rem;
  color: #333;
  font-style: normal;
}

.profile-info__name .status {
  display: inline-block;
  position: relative;
  padding: 0 .25rem;
  color: transparent;
  font-size: .75rem;
  line-height: 1rem;
  background-color: #FF5B5E;
  -webkit-transform: skew(-15deg, 0deg);
  -moz-transform:skew(-15deg, 0deg);
  -o-transform:skew(-15deg, 0deg);
  -ms-transform:skew(-15deg, 0deg);
  transform: skew(-15deg, 0deg);
}

.profile-info__name .status:before {
  content: attr(data-label);
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  transform: translate(-50%, -50%) skew(15deg, 0deg);
  font-size: .75rem;
  line-height: 1rem;
  text-align: center;
  color: #fff;
}

.profile-info__phone span,
.profile-info__phone img {
  vertical-align: middle;
}

.profile-info__phone img {
  vertical-align: sub;
  width: 1rem;
  height: 1rem;
}

.content {
  padding: 1rem;
  padding-bottom: 3.75rem;
  display: flex;
  flex: 1;
  flex-direction: column;
  width: 100%;
  border-top: .625rem solid #f7f7f7;
  background-color: #fff;
}

/* title */
.content .title {
  position: relative;
  width: 100%;
  font-size: .875rem;
  color: #D12527;
  line-height: 1;
  font-weight: bold;
  padding-bottom: 1em;
}

.content .title:before {
  content: "";
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 100vw;
  height: 1px;
  background-color: #f7f7f7;
  display: block;
  transform: translate(-50%, 0);
}

.content .title span {
  position: relative;
}

.content .title span:before {
  content: '';
  position: absolute;
  bottom: -.625em;
  left: 50%;
  width: .75rem;
  height: .125rem;
  background-color: #D12527;
  transform: translate(-50%, 0);
}

/* header */
.header {
  margin-top: .875rem;
  padding: 0 .5rem;
  width: 100%;
}

.record {
  width: 100%;
}

.record-process {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.record-process__axis {
  position: absolute;
  top: .625rem; /* padding + icon height - (line height / 2) */
  left: .935rem; /* padding + icon width * 2 / 3 */
  width: calc(100% - 1.875rem); /* (padding + icon width *2/3) *2 */
  height: .125rem;
  background-color: #F7F7F7;
  transform: translate(0, -50%);
}

.record-process__node {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.record-process__node-icon {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 1.25rem;
  height: 1.25rem;
}

.record-process__node-icon .icon {
  position: relative;
  width: .625rem;
  height: .625rem;
  border: 1px solid #D12527;
  border-radius: 50%;
  background-color: #fff;
}

.record-process__node-icon.active {
  border-radius: 50%;
  background-color: rgba(209,37,39, .28);
}

.record-process__node-icon.active .icon {
  background-color: #D12527;
}

.record-process__node-label {
  margin-top: .325rem;
  color: #555;
  font-size: .75rem;
  line-height: 1em;
}

.record-process__schedule {
  position: absolute;
  top: .625rem; /* padding + icon height - (line height / 2) */
  left: .935rem; /* padding + icon width */
  width: 0px;
  max-width: calc(100% - 1.875rem);
  height: .125rem;
  background-color: #FBE2E3;
  transform: translate(0, -50%);
}

.building {
  margin-top: 1.4rem;
  display: flex;
  width: 100%;
}

.building-cover {
  position: relative;
  width: 21.44%;
  border-radius: .25rem;
}

.building-cover:before {
  content: '';
  display: block;
  padding-top: 64.8648%;
}

.building-cover img {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  object-fit: cover;
}

.building-info {
  margin-left: .75rem;
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: space-between;
  overflow: hidden;
}

.building-info__primary,
.building-info__secondary {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 0;
  width: 100%;
  overflow: hidden;
}

.building-info__primary-name em,
.building-info__primary-reward em {
  color: #333;
  font-size: 1rem;
  font-style: normal;
  text-overflow: ellipsis;
  overflow: hidden;
}

.building-info__primary-name,
.building-info__secondary-address,
.building-info__secondary-address-area {
  display: flex;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}

.building-info__primary-name em,
.building-info__secondary-address-area em {
  width: 100%;
  white-space: nowrap;
  font-style: normal;
}

.building-info__primary-reward {
  margin-left: .75rem;
  font-size: .75rem;
  color: #999;
}

.building-info__secondary {
  font-size: .625rem;
  /* line-height: 1em; */
}

.building-info__secondary-address-area {
  margin-left: .375rem;
}

.building-info__secondary-address-float {
  margin-left: .75rem;
  display: flex;
  align-items: center;
  font-size: .875rem;
  color: #1F99C6;
}

.building-info__secondary-address-float img {
  margin-right: .325rem;
  width: .625rem;
  height: .84rem;
  object-fit: cover;
}

.building-info__secondary-address-float.rise {
  color: #D12527;
}


/* check button */
.pass-the-audit {
  margin-top: 1.5rem;
  width: 100%;
  display: flex;
}

.pass-the-audit__button {
  padding: 0;
  width: 100%;
  font-size: .875rem;
  line-height: 2rem;
  color: #D12527;
  background-color: #FAE9E9;
  margin: 0 0.2rem;
  border: 0;
}
.pass-the-audit__button .buttom1{
  color: #9A25D1;
  background-color: #F9EBFF;
}

.content .line {
  margin-top: 1rem;
  position: relative;
  height: 1px;
}

.content .line:before {
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: -1rem;
  width: 100vw;
  height: 1px;
  background-color: #EBEBEB;
}

/* recording */
.recording {
  margin-top: 1rem;
  display: flex;
  flex-direction: column;
  flex: 1;
}

.recording-list {
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  list-style-type: none;
}

.recording-list__item {
  display: flex;
  flex-direction: row;
}

.recording-list__item-timer p {
  margin-bottom: 0;
  text-align: center;
  color: #D12527;
}

.recording-list__item-timer .day {
  font-size: 20px;
  margin-top: 4px;
}

.recording-list__item-timer .month {
  font-size: 13px;
}

.recording-list__item-line,
.recording-list__item-content {
  position: relative;
  margin-left: .625rem;
  display: flex;
  flex-direction: column;
}

.recording-list__item-line {
  align-items: center;
}

.recording-list__item-line .avatar {
  position: relative;
}

.recording-list__item-line .avatar img {
  width: 2.875rem;
  height: 2.875rem;
  border-radius: 50%;
  border: 1px solid #FBE2E3;
  object-fit: cover;
  overflow: hidden;
  background-color: #fff;
}

.recording-list__item-line .avatar .type {
  padding: 0 .5rem;
  position: absolute;
  left: 50%;
  bottom: .125rem;
  font-size: .625rem;
  white-space: nowrap;
  min-width: 100%;
  height: 1rem;
  line-height: 1rem;
  text-align: center;
  color: #fff;
  border-radius: .625rem;
  background-color: rgba(148, 204, 248, .9);
  overflow: hidden;
  transform: translate(-50%, 0) scale(.9);
}

.recording-list__item-line:before {
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: 50%;
  width: 1px;
  height: 100%;
  background-color: #FBE2E3;
  transform: translate(-50%, 0);
}

.recording-list__item-content {
  flex: 1;
}

.recording-list__item-content p {
  margin: 0;
}

.recording-list__item-content .name {
  font-size: .875rem;
  color: #999;
}

.recording-list__item-content .status {
  margin-top: .325rem;
  font-size: 1rem;
  color: #D12527;
}

.recording-list__item-content .message {
  color: #999;
}

.recording-list__item-content .images {
  margin-top: .325rem;
  padding-bottom: 1.875rem;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}

.recording-list__item-content .images .images-item {
  position: relative;
  margin-right: 4%;
  width: 29.0566%;
  max-width: 154px;
  margin-top: .625rem;
  background-color: #f9f9f9;
}

.recording-list__item-content .images .images-item:before {
  content: '';
  display: block;
  padding-top: 100%;
}

.recording-list__item-content .images .images-item .picture {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.recording-list__item-content .images .images-item:nth-child(-n+3) {
  margin-top: 0;
}

.recording-list__item-content .images .images-item:nth-child(3n) {
  margin-right: 0;
}

/* 移除最后一个的时间线 */
.recording-list__item:last-child .recording-list__item-line:before {
  display: none;
}


.footer {
  padding: .75rem 1rem .5rem 1rem;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  align-items: center;
  border-top: 1px solid #f7f7f7;
  background-color: #fff;
  z-index: 5;
}

.footer__icon-button {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #999;
  font-size: .75rem;
  line-height: 1em;
}

.footer__icon-button img {
  width: 1.375rem;
  height: 1.375rem;
}

.footer .chart {
  margin-left: 2rem;
}

.footer .contact {
  margin-left: auto;
  /*width: 56vw;*/
  width: 100%;
  max-width: 420px;
  height: 2.5rem;
  color: #fff;
  font-size: .825rem;
  line-height: 2.785rem;
  text-align: center;
  border-radius: .25rem;
  background: linear-gradient(10deg,rgba(206,12,14,1),rgba(235,61,0,1));
}
