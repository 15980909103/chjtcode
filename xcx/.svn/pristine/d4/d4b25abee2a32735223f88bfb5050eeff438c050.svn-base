/**app.wxss**/
@import "/dist/common/index.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.button_box{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  background: #ffffff;
  z-index: 1000;

}
.button_box .my-button{
  position: absolute;
  top: 50%;
  left: 0;
  width: 100%;

}
.btn_title{
  text-align: center;
  color: #D12627;
  line-height: 300px;
}