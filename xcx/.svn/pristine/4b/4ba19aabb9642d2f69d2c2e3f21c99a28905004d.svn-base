    border-radius: 20px;
}



#my-footer{width: 100vw;height: 10vh;background-color: #FFF;position: fixed;left: 0;bottom: 0;display:flex;align-items:center;}
.f-name-1 img{width: 4vh;height: 4vh;margin: 0 auto;}
.f-name-2{font-size: 3.3vw;color: #999;line-height: 4vh;text-align: center;}
.f-btn1{width: 35vw;line-height:8vh;border-radius:1vw;margin-left: 8vw;background: linear-gradient(45deg,#eb3d00,#eb6100);color: #FFF;text-align: center;font-size: 3.5vw;}
.f-btn2{width: 26vw;line-height:8vh;background: linear-gradient(45deg,#ec0c0e,#eb3d00);color: #FFF;text-align: center;font-size: 3.5vw;}
.f-btn3{width: 35vw;line-height:8vh;border-radius:1vw;margin-left: 8vw;background: linear-gradient(45deg,#AAAAAA,#AAAAAA);color: #FFF;text-align: center;font-size: 3.5vw;}
#my-pop{width: 80vw;height:30vh;padding: 2vh 5vw;background-color: #FFF;position: absolute;}
.van-popup{border-radius: 1vw;}
.pop-title{font-size: 5vw;font-weight: 600;text-align: center;line-height: 6vh;position: relative;}
.pop-title img{width: 7vw;height: 7vw;position: absolute;right:0;top:0;}
.pop-message{font-size: 3.6vw;text-align: center;color: #999;line-height: 6vh;}
.pop-btn{line-height: 8vh;background-color: #D22527;color: #FFF;text-align: center;border-radius: 1vw;margin-top: 4vh;}
.my-mask{width: 100%;height: 100%;background-color: #000;opacity:.5;position: absolute;top:0;}
.zdy-pop{width: 100vw;height: 100vh;position: fixed;top: 0;left: 0;z-index: 999;display: flex;justify-content: center;align-items: center;}
.f-name-2-tick{color:#D33032;}

.swiper_index{
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    text-align: center;
    color: #ffffff;
    background: rgba(0, 0, 0, .5);
    border-radius: 200px;
    padding: 0 0.5rem;
    box-sizing: content-box;
    font-size: 0.5rem;
}

.view3-li {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.view3-li2 .view3-ad {
    display: flex;
    flex-direction: row;
}
.view3-li2 .view3-dt {
    height: 1rem;
}
.view3-li2 .view3-ad .view3-label {
    white-space: nowrap;
}

/* footer */
#my-footer {
    max-height: 50px;
}
.footer__operating {
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: center;
    height: 100%;
}
.f-name {
    margin-left: 8vw;
}
.f-name-1 img {
    width: 1.375rem;
    height: 1.375rem;
}
.f-name-2 {
    line-height: 1.125em; /* 图片有多余空白 */
}
.footer__button {
    width: 28.54%;
    height: 100%;
    text-align: center;
    color: #fff;
    line-height: 50px;
    font-size: .825rem;
}

.chatroom.footer__button {
    background-color: #eb6100;
}
.f-btn2.footer__button {
    background-color: #ce0c0e;
}

.my-content {
    overflow: hidden;
}

.my-view7 .view7-loudong {
    max-width: 100%;
}

.view6-nav {
    padding: 0;
    overflow: hidden;
    overflow-x: scroll;
    justify-content: flex-start;
}

.view6-nav-li {
    flex-shrink: 0;
    width: 22vw;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.view6-nav:after {
    content: '';
    padding-left: 1rem;
}

:root {
    font-size: 16px;
}

.view7-loudong img {
    object-fit: contain;
}


.report-rules {
    padding: .75rem;
    display: flex;
    justify-content: space-between;
    width: 100vw;
    font-size: 1rem;
    border-top: 2vh solid #F7F7F7;
}
