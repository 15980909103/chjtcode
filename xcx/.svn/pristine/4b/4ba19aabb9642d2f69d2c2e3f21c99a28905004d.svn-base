    width: 100vw;
    display: flex;
    /*overflow: hidden;*/
    overflow-x: scroll;
    overflow-y: hidden;
}
.house-type-pictrue {
    padding-right: 5vw;
    display: flex;
    flex-direction: row;
    box-sizing: border-box;
}

.view5-li{
    display: flex;
    flex-direction: column;
    margin-left: 5vw;
    width:45vw;
    flex-shrink: 0;
    box-sizing: border-box;
}
.view5-li img{width: 45vw;height: 45vw;}
.view5-col1{display: flex;align-items:center;margin-top: 1vh;}
.view5-row1{font-size: 3.6vw;font-weight: 600;}
