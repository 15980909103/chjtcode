.div9-li1{width: 63vw;height: 8vh;padding-left: 5vw;display: flex;flex-direction:column;justify-content:center;}
.div9-li1-1{color: #fff;font-size: 4vw;font-weight: 600;}
.div9-li1-2{color: #fff;opacity: .75;font-size:3.5vw;}


.div9-li2{width: 27vw;height: 8vh;border-left: 1px dashed #fff;position: relative;display: flex;justify-content:center;align-items:center;}
.div9-li2-tl{width: 5vw;height: 5vw;background-color: #FFF;position: absolute;top:-2.5vw;left: -2.5vw;border-radius: 50%;}
