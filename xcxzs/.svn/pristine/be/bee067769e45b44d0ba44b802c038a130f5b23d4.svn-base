        },
		computed: {
		    inputPlaceholder() {
		        let text = '';
		        if (!this.storeType) {
		            text = '客户'
		        } else if (this.permission == 2) {
		            text = '经纪人';
		        } else {
		            text = '组员';
		        }
		
		        return `请输入${text}名字`;
		    },
		},
		watch:{
			tab_active(val){
				// this.fetchListData()
				this.statuslist = []
				if(val == 3){
					this.statuslist = this.processedlist
				}else{
					this.statuslist = [{
						id:	1,
						text: '报备 ',
					},{
						id:	2,
						text: '带看',
					},{
						id:	3,
						text: '成交',
					},{
						id:	4,
						text: '确认业绩 ',
					},{
						id:	5,
						text: '开票',
					},{
						id:	6,
						text: '结佣',
					}]
				}
			},
			checkeall(val){
				this.$refs.checkboxGroup[0].toggleAll(val);
			},
			is_readall(val){
				this.$refs.checkboxGroup2[0].toggleAll(val);
			},
			agentcheckeall(val){
				console.log('val',val)
				console.log('checkboxGroupagent',this.$refs)
				this.$refs.checkboxGroupagent.toggleAll(val);
			}
